{
    "compilerOptions": {
        "allowJs": true,
        "module": "amd",
        "target": "es5",
        "noEmit": false,
        "noEmitOnError": true,
        "noImplicitAny": false,
        "sourceMap": false,
        "watch": true,
        "removeComments": true,
        // "out":"build/map.js"//当module是amd或者system时生效
        "outDir": "build" //commonjs
    },
    "exclude": [
        "node_modules"
    ]
}